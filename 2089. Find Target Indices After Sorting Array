class Solution {
public:
    vector<int> targetIndices(vector<int>& nums, int target) {
        vector<int>s;
        sort(begin(nums),end(nums));
        int size=nums.size();
        
        for(int i=0; i<size; i++)
        {
            if(nums[i]==target)
            s.push_back(i);
        }
        return s;
    }
};
