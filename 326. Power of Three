class Solution {
public:
    bool isPowerOfThree(int n) {
        double a=double (n);
        if(a<=0)
        return false;
        if(a==1)
        return true;
        while(!(a<3))
        {
            a=a/3;
        }
        if(a==1)
        return true; 
        else return false;
    
        
    }
};
